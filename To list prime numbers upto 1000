#include<iostream>
#include<math.h>
using namespace std;
bool isPrime(int n)
{
	if (n<2)
   {return 0;}

   else if(n==2)
   {return 1;}
   
   else if(n%2==0)
   {return 0;}
   
   else
   {for(int i=3;i<sqrt(n);i++)
      {if(n%i==0)
       {return 0;}
	  }
    return 1;
   }
}
int main()

{for(int j=2;j<1000;j++)
{if(isPrime(j)==1)
{cout<<j<<"isPrime"<<endl;}
}
}

